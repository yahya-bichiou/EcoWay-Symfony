
{{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}

<table>
<tr>
    <td> {{ form_label(form.nom, "votre nom et prenom") }} </td>
    <td> {{ form_widget(form.nom) }} </td>
    <td> {{ form_errors(form.nom) }} </td>
</tr>
<tr>
    <td> {{ form_label(form.age, "votre Age") }} </td>
    <td> {{ form_widget(form.age) }} </td>
    <td> {{ form_errors(form.age) }} </td>
</tr>
<tr>
    <td> {{ form_label(form.telephone, "telephone") }} </td>
    <td> {{ form_widget(form.telephone) }} </td>
    <td> {{ form_errors(form.telephone) }} </td>
</tr>
<tr>
    <td> {{ form_label(form.email, "Email") }} </td>
    <td> {{ form_widget(form.email) }} </td>
    <td> {{ form_errors(form.email) }} </td>
</tr>
</table>
<button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}

